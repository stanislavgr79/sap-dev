<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        cq:template="sap/templates/page-home"
        sling:resourceType="sap/components/page">
        <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sap/components/container"
                layout="responsiveGrid"/>
            <root1
                    jcr:primaryType="nt:unstructured">
                <container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="sap/components/container">
                    <hero
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="sap/components/hero/v1/hero"/>
                    <separator
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="sap/components/separator/v1/separator"/>
                </container>
            </root1>
            <root2
                    jcr:primaryType="nt:unstructured">
                <container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="sap/components/container">
                    <multiparsys_1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="sap/components/multiparsys/v1/multiparsys"
                            column="3"
                            typeOfopen="_self">
                        <par_c1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="wcm/foundation/components/parsys">
                            <hero
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="sap/components/hero/v2/hero"/>
                        </par_c1>
                        <par_c2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="wcm/foundation/components/parsys">
                            <hero
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="sap/components/hero/v2/hero"/>
                        </par_c2>
                        <par_c3
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="wcm/foundation/components/parsys">
                            <hero
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="sap/components/hero/v2/hero"/>
                        </par_c3>
                    </multiparsys_1>
                    <separator_1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="sap/components/separator/v2/separator"/>
                    <multiparsys_2
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="sap/components/multiparsys/v1/multiparsys"
                            column="3"
                            typeOfopen="_self">
                        <par_c1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="wcm/foundation/components/parsys">
                            <news
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="sap/components/news/v1/news"/>
                        </par_c1>
                        <par_c2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="wcm/foundation/components/parsys">
                            <news
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="sap/components/news/v1/news"/>
                        </par_c2>
                        <par_c3
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="wcm/foundation/components/parsys">
                            <news
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="sap/components/news/v1/news"/>
                        </par_c3>
                        <separator_1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="sap/components/separator/v1/separator"/>
                    </multiparsys_2>
                </container>
            </root2>
            <root3
                    jcr:primaryType="nt:unstructured">
                <container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="sap/components/container"/>
            </root3>
        </jcr:content>
</jcr:root>

